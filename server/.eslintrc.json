{
    "root": true,
    "parserOptions": {
      "ecmaVersion": 12,
      "sourceType": "module"
    },
    "extends": ["eslint:recommended", "prettier"],
    "plugins": ["prettier"],
    "env": {
      "es2021": true,
      "node": true,
      "jest": true
    },
    "rules": {
      "no-console": "error",
      "no-unused-vars": "off"
    }
  }